{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": ["http://localhost:8080"]
  },
  "local": true,
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-set-size",
    "core:window:allow-set-min-size",
    "core:window:allow-start-dragging"
  ]
}
